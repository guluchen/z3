(set-logic ALL)
(declare-const key String)
(declare-const value String)
(assert (not (< (str.len key) 0)))
(assert (str.prefixof "/" key))
(assert (str.contains key "="))
(assert (not (< (str.len value) 0)))
(assert (= (+ 0 (str.indexof value "\n" 0)) (- 1)))
(assert (not (< (str.len (str.++ "" value)) 0)))
(assert (= (+ 0 (str.indexof (str.++ "" value) "\r" 0)) (- 1)))
(assert (str.contains key "translation"))
(assert (not (< (str.len (str.++ "" (str.++ "" value))) 0)))
(assert (= (+ 0 (str.indexof (str.++ "" (str.++ "" value)) " " 0)) (- 1)))
(assert (not (< (str.len (str.++ "" (str.++ "" (str.++ "" value)))) 0)))
(assert (= (+ 0 (str.indexof (str.++ "" (str.++ "" (str.++ "" value))) " " 0)) (- 1)))
(assert (not (not (= (str.++ "" (str.++ "" (str.++ "" value))) ""))))
(check-sat)
(get-value (key))
(get-value (value))
