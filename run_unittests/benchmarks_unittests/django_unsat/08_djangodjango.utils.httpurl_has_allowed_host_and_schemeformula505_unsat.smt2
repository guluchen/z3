(set-logic ALL)
(declare-const url String)
(declare-const allowed_hosts String)
(declare-const require_https Bool)
(assert (not (= url "")))
(assert (not (< (str.len url) 0)))
(assert (not (str.prefixof "///" url)))
(assert (not (< (str.len url) 0)))
(assert (not (> (+ 0 (str.indexof url ":" 0)) 0)))
(assert (not (= (str.substr url 0 (- 2 0)) "//")))
(assert (str.contains url "#"))
(assert (not (< (str.len url) 0)))
(assert (not (= (+ 0 (str.indexof url "#" 0)) (- 1))))
(assert (not (< (+ 0 (str.indexof url "#" 0)) 0)))
(assert (not (< (+ (+ 0 (str.indexof url "#" 0)) 1) 0)))
(assert (not (< (str.len url) 0)))
(assert (str.contains (str.substr url 0 (- (+ 0 (str.indexof url "#" 0)) 0)) "?"))
(assert (not (< (str.len (str.substr url 0 (- (+ 0 (str.indexof url "#" 0)) 0))) 0)))
(assert (not (= (+ 0 (str.indexof (str.substr url 0 (- (+ 0 (str.indexof url "#" 0)) 0)) "?" 0)) (- 1))))
(assert (< (+ 0 (str.indexof (str.substr url 0 (- (+ 0 (str.indexof url "#" 0)) 0)) "?" 0)) 0))
(check-sat)
(get-value (url))
(get-value (allowed_hosts))
(get-value (require_https))
