(set-logic ALL)
(declare-const element String)
(declare-const border String)
(assert (not (< (str.len border) 0)))
(assert (not (= (+ 0 (str.indexof border " " 0)) (- 1))))
(assert (not (< (+ 0 (str.indexof border " " 0)) 0)))
(assert (not (< (+ (+ 0 (str.indexof border " " 0)) 1) 0)))
(assert (not (< (str.len border) 0)))
(assert (< (str.len (str.substr border (+ (+ 0 (str.indexof border " " 0)) 1) (- (str.len border) (+ (+ 0 (str.indexof border " " 0)) 1)))) 0))
(check-sat)
(get-value (element))
(get-value (border))
