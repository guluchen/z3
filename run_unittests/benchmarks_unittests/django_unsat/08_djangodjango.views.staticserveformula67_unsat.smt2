(set-logic ALL)
(declare-const request String)
(declare-const path String)
(declare-const show_indexes Bool)
(assert (not (= path "")))
(assert (not (< (str.len path) 0)))
(assert (not (str.prefixof "/" path)))
(assert (not (< (str.len path) 0)))
(assert (not (= (+ 0 (str.indexof path "/" 0)) (- 1))))
(assert (not (< (+ 0 (str.indexof path "/" 0)) 0)))
(assert (not (< (+ (+ 0 (str.indexof path "/" 0)) 1) 0)))
(assert (not (< (str.len path) 0)))
(assert (not (< (str.len (str.substr path (+ (+ 0 (str.indexof path "/" 0)) 1) (- (str.len path) (+ (+ 0 (str.indexof path "/" 0)) 1)))) 0)))
(assert (not (= (+ 0 (str.indexof (str.substr path (+ (+ 0 (str.indexof path "/" 0)) 1) (- (str.len path) (+ (+ 0 (str.indexof path "/" 0)) 1))) "/" 0)) (- 1))))
(assert (not (< (+ 0 (str.indexof (str.substr path (+ (+ 0 (str.indexof path "/" 0)) 1) (- (str.len path) (+ (+ 0 (str.indexof path "/" 0)) 1))) "/" 0)) 0)))
(assert (< (+ (+ 0 (str.indexof (str.substr path (+ (+ 0 (str.indexof path "/" 0)) 1) (- (str.len path) (+ (+ 0 (str.indexof path "/" 0)) 1))) "/" 0)) 1) 0))
(check-sat)
(get-value (request))
(get-value (path))
(get-value (show_indexes))
