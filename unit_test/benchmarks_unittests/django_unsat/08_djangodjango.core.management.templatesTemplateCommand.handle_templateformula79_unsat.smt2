(set-logic ALL)
(declare-const template String)
(declare-const subdir String)
(assert (not (< (str.len template) 0)))
(assert (not (str.prefixof "file://" template)))
(assert (not (< (str.len template) 0)))
(assert (not (str.prefixof "~" template)))
(assert (not (= template "")))
(assert (not (< (str.len template) 0)))
(assert (str.prefixof "/" template))
(assert (not (< (str.len template) 0)))
(assert (not (str.prefixof "//" template)))
(assert (not (< (str.len template) 0)))
(assert (not (= (+ 0 (str.indexof template "/" 0)) (- 1))))
(assert (not (< (+ 0 (str.indexof template "/" 0)) 0)))
(assert (< (+ (+ 0 (str.indexof template "/" 0)) 1) 0))
(check-sat)
(get-value (template))
(get-value (subdir))
